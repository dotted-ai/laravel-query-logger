{
    "name": "dottedai/laravel-query-logger",
    "description": "A package to log and display database queries like Rails console.",
    "type": "library",
    "require": {
      "php": ">=7.2",
      "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
      "psr-4": {
        "Dottedai\\QueryLogger\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Dottedai\\QueryLogger\\QueryLoggerServiceProvider"
        ]
      }
    }
  }
  